.row-fluid.assignments
  .span12
    %h3 제출 목록
    .title-bar-thin
    .title-bar-thick
.row-fluid
  .span4
    %h4 주제
    %select
      %option 이거슨 샘플입니다
      %option 이것도 샘플입니다
      %option This is Sample
      %option Is this sample??
  .span4
    %h4 문제
    %select
      %option 샘플의 귀환
      %option 리턴 오브 샘플
      %option 노모어 샘플
    %a.btn.btn-primary.move-10px-to-top{href:"#"} 검색
  .span4
.row-fluid.assignments
  .span12  
    %table.table.table-striped
      %thead
        %tr
          %th 제출번호
          %th 상태
          %th 소요시간
          %th 메모리 사용량
          %th 결과보기
      %tbody
        %tr.success
          %td 1
          %td Success 
          %td 105ms
          %td 100KB
          %td=link_to "결과보기" , "#", class: 'btn btn-info'
        %tr.error
          %td 2
          %td Fail 
          %td 24ms
          %td 10KB
          %td=link_to "결과보기" , "#", class: 'btn btn-info'

.blank
